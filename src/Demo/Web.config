<configuration>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
    <!-- ##########################AliPay交易发送地址配置#############################-->
    <add key="alipay.partner" value="" />
    <!-- 安全校验码 -->
    <add key="alipay.key" value="" />
    <!-- 获取或设置签约支付宝账号或卖家支付宝帐户 -->
    <add key="alipay.seller_email" value="" />
    <!-- APP支付接口appid -->
    <add key="alipay.appid" value="" />
    <!-- 获取页面跳转同步通知页面路径 -->
    <add key="alipay.return_url" value="" />
    <!-- 获取服务器异步通知页面路径 -->
    <add key="alipay.notify_url" value="" />
    <!-- ##########################WeChatPayment交易发送地址配置#############################-->
    <!-- 获取或设置合作者身份ID -->
    <add key="wechat.mch_id" value="" />
    <add key="wechat.appid" value="" />
    <!--安全校验码 -->
    <add key="wechat.key" value="" />
    <!-- 获取服务器异步通知页面路径 -->
    <add key="wechat.return_url" value="" />
    <add key="wechat.notify_url" value="" />
    <!-- ##########################UnionPay交易发送地址配置#############################-->
    <!--  签名证书路径 （联系运营获取两码，在CFCA网站下载后配置，自行设置证书密码并配置） -->
    <add key="sdk.signCert.path" value="" />
    <!-- 签名证书密码 -->
    <add key="sdk.signCert.pwd" value="" />
    <!-- 签名证书类型 -->
    <add key="sdk.signCert.type" value="PKCS12" />
    <!-- 加密证书路径 -->
    <add key="sdk.encryptCert.path" value="" />
    <!-- 验签证书目录 -->
    <add key="sdk.validateCert.dir" value="" />
    <!-- 前台交易地址 -->
    <add key="sdk.frontTransUrl" value="https://gateway.95516.com/gateway/api/frontTransReq.do" />
    <!-- 后台交易地址 -->
    <add key="sdk.backTransUrl" value="https://gateway.95516.com/gateway/api/backTransReq.do" />
    <!-- 交易状态查询地址 -->
    <add key="sdk.singleQueryUrl" value="https://gateway.95516.com/gateway/api/queryTrans.do" />
    <!-- 文件传输类交易地址 -->
    <add key="sdk.fileTransUrl" value="https://filedownload.95516.com/" />
    <!-- 批量交易地址 -->
    <add key="sdk.batTransUrl" value="https://gateway.95516.com/gateway/api/batchTrans.do" />
    <!-- 有卡交易地址 -->
    <add key="sdk.cardRequestUrl" value="https://gateway.95516.com/gateway/api/cardTransReq.do" />
    <!-- app交易地址 手机控件支付使用该地址-->
    <add key="sdk.appRequestUrl" value="https://gateway.95516.com/gateway/api/appTransReq.do" />
    <!-- 前台通知地址，填写后台接收银联前台通知的地址 -->
    <add key="frontUrl" value="" />
    <!-- 后台通知地址，填写后台接收银联后台通知的地址，必须外网能访问 -->
    <add key="backUrl" value="" />
    <!-- 是否验证服务器https证书 -->
    <add key="ifValidateRemoteCert" value="false" />
    <!-- 商户号 -->
    <add key="merId" value="802440348160649" />
    <!-- ##########################Tenpay交易发送地址配置#############################-->
    <!--获取或设置合作者身份ID-->
    <!--<add key="tenpay.partner" value="" /> -->
    <!--安全校验码-->
    <!--<add key="tenpay.key" value="" />-->
    <!--获取页面跳转同步通知页面路径-->
    <!--<add key="tenpay.return_url" value="" />-->
    <!--获取服务器异步通知页面路径-->
    <!--<add key="tenpay.notify_url" value="" />-->
